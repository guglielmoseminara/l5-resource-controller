{
    "name": "guglielmoseminara/l5-resource-controller",
    "description": "Resource Controller for Laravel 5",
    "version": "v2.1.0",
    "keywords": [
        "resource controller", "laravel 5"
    ],
    "homepage": "https://github.com/guglielmoseminara/l5-resource-controller",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Patronelli",
            "email": "mario@raffles.com.ar",
            "homepage": "http://raffles.com.ar",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2.5",
        "laravel/framework": "7.*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "orchestra/database": "^5.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "RafflesArgentina\\ResourceController\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RafflesArgentina\\ResourceController\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
